task.spawn(function()
	local colors = {
		Color3.fromRGB(255, 0, 0),
		Color3.fromRGB(255, 255, 0),
		Color3.fromRGB(0, 0, 255),
		Color3.fromRGB(128, 0, 128),
		Color3.fromRGB(255, 165, 0),
		Color3.fromRGB(0, 255, 0),
		Color3.fromRGB(255, 51, 153),
	}

	local currentIndex = 1
	local nextIndex = 2

	while true do
		local currentColor = colors[currentIndex]
		local nextColor = colors[nextIndex]

		for step = 0, 1, 0.02 do
			local lerped = currentColor:Lerp(nextColor, step)
			OpenUtg.TextStrokeColor3 = lerped
			subtext.TextStrokeColor3 = lerped
			wait(0.03)
		end

		currentIndex = nextIndex
		nextIndex = (nextIndex % #colors) + 1
	end
end)

AntiFling.Text = game.Players.LocalPlayer.Name

AntiFling.FocusLost:Connect(function()
	if AntiFling.Text == "Disable" then
		AntiFling:Destroy()
		return
	end

	for _, player in pairs(game.Players:GetPlayers()) do
		if string.find(player.Name:lower(), AntiFling.Text:lower()) then
			AntiFling.Text = player.Name
			break
		end
	end
end)

local function MakeDraggable()
	local UIS = game:GetService('UserInputService')
	local frame = Main
	local dragToggle = false
	local dragSpeed = 0.25
	local dragStart = nil
	local startPos = nil

	local function updateInput(input)
		local delta = input.Position - dragStart
		local position = UDim2.new(
			startPos.X.Scale, startPos.X.Offset + delta.X,
			startPos.Y.Scale, startPos.Y.Offset + delta.Y
		)
		game:GetService('TweenService'):Create(frame, TweenInfo.new(dragSpeed), {Position = position}):Play()
	end
	frame.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragToggle = true
			dragStart = input.Position
			startPos = frame.Position

			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragToggle = false
				end
			end)
		end
	end)

	UIS.InputChanged:Connect(function(input)
		if (input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch) and dragToggle then
			updateInput(input)
		end
	end)
end
MakeDraggable()
Main:TweenPosition(UDim2.new(0.382, 0, 0.181, 0), "Out", "Quad", 0.5, true)
for i = 1,500 do 
	print("ULTIMATE TROLLING GUI HAS LOADED!")
end
