--=={ Startup Script }==--

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local PlayerGui = LocalPlayer:WaitForChild("PlayerGui")

local LAMO = Instance.new("ScreenGui")
LAMO.Name = "LAMO_TypingEffect"
LAMO.IgnoreGuiInset = true
LAMO.DisplayOrder = 1000
LAMO.ResetOnSpawn = false
LAMO.Parent = PlayerGui

local Frame = Instance.new("Frame")
Frame.Name = "BlackoutFrame"
Frame.Parent = LAMO
Frame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
Frame.BorderSizePixel = 0
Frame.Position = UDim2.new(0, 0, 0, 0)
Frame.Size = UDim2.new(1, 0, 1, 0)

local music = Instance.new("Sound")
music.Name = "IntroMusic"
music.SoundId = "rbxassetid://137820949448461"
music.Volume = 2
music.PlaybackSpeed = 1
music.Looped = false
music.Parent = workspace
music:Play()

local TextLabel = Instance.new("TextLabel")
TextLabel.Parent = Frame
TextLabel.Size = UDim2.new(0.6, 0, 0.5, 0)
TextLabel.Position = UDim2.new(0.5, 0, 0.55, 0)
TextLabel.AnchorPoint = Vector2.new(0.5, 0.5)
TextLabel.BackgroundTransparency = 1
TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
TextLabel.Font = Enum.Font.Arcade
TextLabel.TextSize = 20
TextLabel.TextXAlignment = Enum.TextXAlignment.Center
TextLabel.TextYAlignment = Enum.TextYAlignment.Center
TextLabel.TextWrapped = true
TextLabel.Text = ""

local NumberImage = Instance.new("ImageLabel")
NumberImage.Name = "NumberImage"
NumberImage.Parent = Frame
NumberImage.Size = UDim2.new(0, 250, 0, 210)
NumberImage.Position = UDim2.new(0.5, 0, 0.35, 0)
NumberImage.AnchorPoint = Vector2.new(0.5, 0.5)
NumberImage.BackgroundTransparency = 1
NumberImage.Image = "rbxassetid://134169990445330"
NumberImage.ScaleType = Enum.ScaleType.Fit

local message = [[
You've always been a killer...In another life, in another game!
You've always known it about that urge, you just tried to bury it.
You've been wearing a mask your whole life...
It's time to embrace it.
this is who you really are.
]]

local delayBetweenChars = 0.06
local running = true
local swap = true

task.spawn(function()
	while running do
		NumberImage.Image = swap and "rbxassetid://134169990445330" or "rbxassetid://95323780549355"
		swap = not swap
		wait(0.5)
	end
end)

for i = 1, #message do
	local currentText = string.sub(message, 1, i)
	TextLabel.Text = currentText

	if currentText:find("this is who you really are") then
		TextLabel.TextColor3 = Color3.fromRGB(255, 0, 0)
	else
		TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
	end

	wait(delayBetweenChars)
end

running = false
wait(1)
NumberImage:Destroy()
Frame:Destroy()

--=={ GUI Work }==--

local Players = game:GetService("Players")
local TweenService = game:GetService("TweenService")
local UserInputService = game:GetService("UserInputService")

local Player = Players.LocalPlayer
local PlayerGui = Player:WaitForChild("PlayerGui")

local screenGui = Instance.new("ScreenGui")
screenGui.Name = "RicksRetributionGUI"
screenGui.Parent = PlayerGui

local frame = Instance.new("Frame")
frame.Name = "RootFrame"
frame.Parent = screenGui
frame.BackgroundTransparency = 1
frame.Size = UDim2.new(1, 0, 1, 0)

local controlFrame = Instance.new("Frame")
controlFrame.Name = "ControlFrame"
controlFrame.Parent = frame
controlFrame.Size = UDim2.new(0, 290, 0, 460)
controlFrame.Position = UDim2.new(1, -5, 1, -5) -- initial on-screen position
controlFrame.AnchorPoint = Vector2.new(1, 1)
controlFrame.BackgroundTransparency = 0.5
controlFrame.BackgroundColor3 = Color3.fromRGB(38, 38, 38)
controlFrame.BorderSizePixel = 0

local function createLabel(parent, text, yOffset)
	local label = Instance.new("TextLabel")
	label.Name = "Label_" .. text:gsub(" ", "_")
	label.Parent = parent
	label.BackgroundTransparency = 1
	label.TextSize = 15
	label.Text = text 
	label.Font = Enum.Font.Legacy
	label.TextColor3 = Color3.new(1, 1, 1)
	label.Size = UDim2.new(0.9, 0, 0, 40)
	label.Position = UDim2.new(0.05, 0, 0, yOffset)
	label.TextXAlignment = Enum.TextXAlignment.Center
	return label
end

local labels = {
	"Rick's Retribution",
	"Created By: YepImSirPwnsAlot",
	"Click - Punch",
	"C - Crouch",
	"K - Kick",
	"J - Jump Kick",
	"U - Uppercut",
	"G - Grab and Throw",
	"R - Slide Kick",
	"F - Get Shotgun",
	"Press L To Hide This Gui"
}

for i, text in ipairs(labels) do
	createLabel(controlFrame, text, 5 + ((i - 1) * 40))
end

local function Tween(object, properties, time, style, dir, repeats, reverse, delay)
	local info = TweenInfo.new(
		time or 1,
		style or Enum.EasingStyle.Linear,
		dir or Enum.EasingDirection.Out,
		repeats or 0,
		reverse or false,
		delay or 0
	)
	local tween = TweenService:Create(object, info, properties)
	tween:Play()
	return tween
end

local InfoHidden = false

UserInputService.InputEnded:Connect(function(input, gameProcessed)
	if gameProcessed then return end
	if input.KeyCode == Enum.KeyCode.L then
		InfoHidden = not InfoHidden
		local targetPosition = InfoHidden and UDim2.new(1, 300, 1, -5) or UDim2.new(1, -5, 1, -5)
		Tween(controlFrame, { Position = targetPosition }, 0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
	end
end)

local music2 = Instance.new("Sound")
music2.Parent = workspace
music2.SoundId = "rbxassetid://123404399362375"
music2.Volume = 5
music2.PlaybackSpeed = 1
music2.Looped = false
music2:Play()

--=={ Character Script }==--

local p = game.Players.LocalPlayer
local player = p
local m = p:GetMouse()
local ch = p.Character or p.CharacterAdded:Wait()
local root = ch:WaitForChild("HumanoidRootPart")
local hum = ch:WaitForChild("Humanoid")

local userinput = game:GetService("UserInputService")
local debris = game:GetService("Debris")

local anim = "Idle"
local attack = false
local grabbed = false
local canplay = false
local canplaying = false
local sine = 0
local change = 1
local speed = 15
local jpower = 75

Instance.new("ForceField", ch).Visible = false

for i, v in pairs(ch:GetChildren()) do
	if v:IsA("Accessory") or v:IsA("CharacterMesh") or v:IsA("BodyColors") or v:IsA("Shirt") or v:IsA("ShirtGraphic") or v:IsA("Pants") then
		v:Destroy()
	end
end

local function weldParts(part0, part1, c0, c1)
	local weld = Instance.new("Weld")
	weld.Part0 = part0
	weld.Part1 = part1
	weld.C0 = c0 or CFrame.new()
	weld.C1 = c1 or CFrame.new()
	weld.Parent = part0
	return weld
end

weldParts(ch.Torso, ch.Head, CFrame.new(0, 1.5, 0))
weldParts(ch.Torso, ch["Right Arm"], CFrame.new(1.5, 0.5, 0), CFrame.new(0, 0.5, 0))
weldParts(ch.Torso, ch["Left Arm"], CFrame.new(-1.5, 0.5, 0), CFrame.new(0, 0.5, 0))
weldParts(ch.Torso, ch["Right Leg"], CFrame.new(0.5, -1, 0), CFrame.new(0, 1, 0))
weldParts(ch.Torso, ch["Left Leg"], CFrame.new(-0.5, -1, 0), CFrame.new(0, 1, 0))
weldParts(root, ch.Torso)

local neck = ch.Torso:FindFirstChildWhichIsA("Weld", true)
local torso = root:FindFirstChildWhichIsA("Weld", true)
local rs = ch.Torso:FindFirstChild("RightShoulder") or weldParts(ch.Torso, ch["Right Arm"])
local ls = ch.Torso:FindFirstChild("LeftShoulder") or weldParts(ch.Torso, ch["Left Arm"])
local rh = ch.Torso:FindFirstChild("RightHip") or weldParts(ch.Torso, ch["Right Leg"])
local lh = ch.Torso:FindFirstChild("LeftHip") or weldParts(ch.Torso, ch["Left Leg"])

local newcolor = Instance.new("BodyColors", ch)
newcolor.HeadColor = BrickColor.new("Cork")
newcolor.LeftArmColor = BrickColor.new("Cork")
newcolor.RightArmColor = BrickColor.new("Cork")
newcolor.LeftLegColor = BrickColor.new("Earth green")
newcolor.RightLegColor = BrickColor.new("Earth green")
newcolor.TorsoColor = BrickColor.new("Earth green")

local pants = Instance.new("Pants", ch)
pants.PantsTemplate = "rbxassetid://11894449582"

for i = 0, 5, 0.1 do
	game:GetService("RunService").Heartbeat:Wait()

	if anim == "Idle" then
		local t = 0.1

		neck.C0 = neck.C0:Lerp(CFrame.new(-0.5, 4.5, -13.4) * CFrame.Angles(0, 0, 0), t)
		torso.C0 = torso.C0:Lerp(CFrame.new(-0.5, 3, -13) * CFrame.Angles(math.rad(-20), 0, 0), t)

		rs.C0 = rs.C0:Lerp(CFrame.new(-2, 3, -13) * CFrame.Angles(math.rad(14.9), 0, 0), t)
		ls.C0 = ls.C0:Lerp(CFrame.new(1, 3, -13.301) * CFrame.Angles(math.rad(14.9), 0, 0), t)

		rh.C0 = rh.C0:Lerp(CFrame.new(0, 1, -12.801) * CFrame.Angles(math.rad(-5.1), 0, 0), 0.3)
		lh.C0 = lh.C0:Lerp(CFrame.new(-1, 1, -13) * CFrame.Angles(math.rad(9.9), 0, 0), 0.3)
	end
end
end)

